<?xml version="1.0"?>
<grammar xmlns="http://www.w3.org/2001/06/grammar" xml:lang="ru-RU" version="1.0" mode="voice" root="digit">
    <rule id="digit">
        <one-of>
            <item><ruleref special="GARBAGE"/><ruleref uri="#source"/><ruleref special="GARBAGE"/><ruleref uri="#buy"/><ruleref special="GARBAGE"/><ruleref uri="#estate"/></item>
