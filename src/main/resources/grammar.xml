            <item>сдаче</item>
            <item>заселении</item>
            <item>заселиться</item>
            <item>заселяться</item>
            <item>заселение</item>
            <item>сдача</item>
            <item>выдача</item>
            <item>ключ</item>
        </one-of>
    </rule>
    <rule id="buy">
        <one-of>
            <item>купить</item>
