            <item><ruleref special="GARBAGE"/><ruleref uri="#transfer"/></item>
        </one-of>
    </rule>
    <rule id="estate">
        <one-of>
            <item>новогиреевский</item>
            <item>новогиреевском</item>
            <item>балашиха</item>
            <item>балашихе</item>
            <item>сколсковский</item>
