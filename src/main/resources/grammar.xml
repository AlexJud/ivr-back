        </one-of>
    </rule>
    <rule id="transfer">
        <one-of>
            <item>перевод</item>
            <item>перевести</item>
            <item>соединить</item>
            <item>соедините</item>
            <item>соедини</item>
            <item>переведи</item>
        </one-of>
    </rule>
    <rule id="source">
        <one-of>
            <item>интернет</item>
            <item>перевести</item>
            <item>соединить</item>
            <item>соедините</item>
