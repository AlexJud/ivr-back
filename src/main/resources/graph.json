    "edgeList": [
      {
        "id": "buy_validate"
      }
    ]
  },
  {
    "id": "buy_ext_name",
    "type": "EXTRACT",
    "props": {
      "varName": "person",
      "rawVarName": "rawPerson",
      "matchFile": "russian_name.txt"
    },
    "edgeList": [
      {
        "id": "buy_validate"
      }
    ]
  },
  {
    "id": "buy_ext_source",
