          "сдача",
          "ключи",
          "выдача",
          "ключей",
          "сдаче",
          "выдаче",
          "ключ"
        ]
      },
      {
        "id": "transfer_ask_number",
        "match": [
          "переведите",
          "перевести",
          "соедини",
          "соединить",
          "соедините"
        ]
      },
      {
