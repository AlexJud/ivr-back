          "varName": "person",
          "rawVarName": "rawPerson",
          "edgeIfEmpty": "buy_ask_name"
        },
        {
          "varName": "estate",
          "rawVarName": "rawEstate",
          "edgeIfEmpty": "buy_ask_estate"
        },
        {
          "varName": "source",
          "rawVarName": "rawSource",
          "edgeIfEmpty": "buy_ask_source"
        }
      ],
      "edgeIfSuccess": "send"
    }
  },
  {
    "id": "buy_ask_name",
